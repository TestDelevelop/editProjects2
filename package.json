{
	"name": "fifth",
	"version": "1.0.15",
	"description": "EC Fashion System CLONA",
	"author": "PRECS.inc",
	"license": "ISC",
	"prettier": {},
	"browserslist": [
		"last 2 versions",
		"> 1% in JP",
		"iOS >= 13",
		"Android >= 7.0",
		"maintained node versions",
		"not IE 11",
		"not dead"
	],
	"engines": {
		"node": "10.23.0",
		"npm": "6.4.1"
	},
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"dev": "run-s checkPackageJson build:watch",
		"prod": "run-s checkPackageJson build:prod",
		"build": "run-s checkPackageJson fix:scss build:prod",
		"build:prod": "cross-env NODE_ENV=production gulp prod",
		"build:watch": "cross-env NODE_ENV=development gulp dev",
		"build:force": "cross-env NODE_ENV=production gulp forceUpdate",
		"build:release": "cross-env RELEASE_MODE=release NODE_ENV=production gulp forceUpdate",
		"build:dev": "cross-env NODE_ENV=development gulp buildCode",
		"build:js": "cross-env NODE_ENV=production gulp buildJs",
		"build:scss": "cross-env NODE_ENV=production gulp buildSass",
		"build:sprite": "run-p build:sprite:*",
		"build:sprite:svg": "cross-env NODE_ENV=production gulp buildSpriteSvg",
		"build:sprite:pc": "cross-env NODE_ENV=production gulp buildSpritePc",
		"build:sprite:sp": "cross-env NODE_ENV=production gulp buildSpriteSp",
		"fix": "run-p fix:*",
		"fix:scss": "stylelint --syntax scss --fix \"**/src/**/*.scss\"",
		"fix:js": "eslint --fix \"**/src/**/*.js\"",
		"lint": "run-p lint:*",
		"lint:scss": "stylelint --syntax scss \"**/src/**/*.scss\"",
		"lint:js": "eslint \"**/src/**/*.js\"",
		"lint:spellCheck": "cspell --no-progress --no-summary --relative",
		"checkBuildMode": "babel-node ./gulpfile.babel.js/scripts/checkBuildMode.js",
		"updateBuildMode": "babel-node ./gulpfile.babel.js/scripts/updateBuildMode.js -- $npm_config_type",
		"checkPackageJson": "babel-node ./gulpfile.babel.js/scripts/detectingChangesToPackageJson.js",
		"help": "babel-node ./gulpfile.babel.js/scripts/help.js",
		"setup": "babel-node ./gulpfile.babel.js/scripts/setup.js",
		"moduleCheck": "npx npm-check-updates",
		"moduleCheck:update": "npx npm-check-updates -u",
		"prepare": "husky install",
		"commit": "cz",
		"addCommitTitle": "babel-node ./gulpfile.babel.js/scripts/addCommitTitle.js"
	},
	"devDependencies": {
